{

  "TunnelConfig": {
    "AuthToken": "your-secret-tunnel-token",
    "MaxRequestSize": 10485760, // 10MB
    "MaxConcurrentRequests": 100
  },
  "PGROKServer": {
    "Routes": {
      "route1": {
        "ClusterId": "pgrokclient",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "pgrokclient": {
        "Destinations": {
          "destination1": {
            "Address": "wss://pgrokclient"
          }
        }
      }
    }
  }
}
